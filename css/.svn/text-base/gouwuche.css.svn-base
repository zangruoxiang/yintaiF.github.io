#gouwuche-box {
	width: 100%;
	background: #eee;
	padding-top: 20px;
}

#gouwuche {
	width: 980px;
	margin: 0 auto;
	background: #FFFFFF;
}

#gouwucheTop {
	width: 980px;
	height: 110px;
}

.gowucheImg1 {
	float: left;
	margin-top: 20px;
	margin-left: 20px;
}

.gowucheImg2 {
	float: right;
	margin-right: 36px;
	margin-top: 25px;
}

#gouwucheCen {
	width: 980px;
	min-height: 327px;
	height: auto!important;
	height: 327px;
	background: #fff;
	position: relative;
}

#gouwucheBot {
	width: 980px;
	height: 381px;
	background: #fff
}

.shoplist {
	width: 100%;
	height: auto;
}

.shop-item {
	width: 100%;
	/*border: 1px solid #d1d1d1;*/
	margin-left: 20px;
	text-align: center;
}

.fl {
	float: left;
}

.shop-item .f2 {
	padding-top: 70px;
	width: 159px;
	display: block;
	font-style: normal;
	height: 100px;
	border: 1px solid #d1d1d1;
	margin-left: -1px;
	background: #f9f9f9;
}

.shop-item .fl {
	background: #f9f9f9;
	padding: 10px;
	width: 135px;
	display: block;
	font-style: normal;
	height: 150px;
	border: 1px solid #d1d1d1;
	margin-left: -1px;
}

.shop-item .fl:after {
	content: "";
	display: inline-block;
	vertical-align: middle;
	height: 100%;
}

.shop-item .count> span {
	display: inline-block;
	width: 30px;
	height: 30px;
	border: 1px solid #d1d1d1;
	line-height: 30px;
	cursor: pointer;
	margin-left: 5px;
}

.delBtn:hover {
	color: red;
	cursor: pointer;
}

.gengduo {
	margin-top: 20px;
	margin-bottom: 20px;
	margin-left: 20px;
}

.gouT ul {
	width: 940px;
	height: 30px;
	background: #666;
	margin: 0 auto;
}

.gouT li {
	float: left;
	color: #fff;
	width: 150px;
	text-align: center;
	font-size: 16px;
	line-height: 30px;
	;
}

.gouT p {
	margin-left: 20px;
	width: 900px;
	height: 30px;
	font-size: 16px;
	line-height: 30px;
	background: #eee;
	padding-left: 40px;
}

.gouT img {
	position: absolute;
	right: 20px;
	bottom: 15px;
	cursor: pointer;
}

#gouwucheBot ol {
	width: 980px;
	height: 32px;
	background: #F4F4F4;
	border-bottom: 1px solid #e2e2e2;
}

#gouwucheBot ol li {
	float: left;
	height: 32px;
	padding: 0 10px;
	line-height: 32px;
	border-right: 1px solid #E2E2E2;
	font-size: 12px;
	color: #666
}

#gouwucheBot ul {
	width: 938px;
	height: 300px;
	padding-top: 30px;
}

#gouwucheBot ul li {
	float: left;
	width: 200px;
	height: 300px;
	border: 1px solid #666;
	margin-right: 10px;
	margin-left: 20px;
}

#gouwucheBot ul li img {
	width: 200px;
	height: 300px;
}








