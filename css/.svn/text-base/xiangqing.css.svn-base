#xiangqing-box {
	width: 100%;
	background: #eee;
	padding-top: 20px;
	padding-bottom: 20px;
	;
}

#xiangqing {
	width: 1190px;
	margin: 0 auto;
	background: #FFFFFF;
	height: 4723px;
}

#xiangqingTop {
	height: 538px;
	width: 1190px;
	background: #f4f4f4;
}

#xiangqingTop-top {
	height: 46px;
}

#xiangqingTop-top ul {
	margin-left: 20px;
	padding-top: 17px;
}

#xiangqingTop-top ul li {
	float: left;
	margin-right: 5px;
}

#xiangqingTop-top ul li a {
	font-size: 12px;
}

#xiangqingTop-bottom {
	height: 442px;
	width: 1190px;
	background: #FFF;
	position: relative;
}

.xiangqingTleft {
	position: absolute;
	top: 40px;
	right: 30px;
	;
}

#xiangqingCenter {
	position: relative
}

#xiangqingCenter ol {
	float: left;
}

#xiangqingCenter ol li {
	float: left;
	margin-top: 10px;
}

.xiangqingCenterR {
	right: 0;
	top: 10px;
	position: absolute;
	z-index: 999;
}

#xiangqingCenter ol li {
	width: 120px;
	height: 30px;
	background: #999;
	text-align: center;
	line-height: 30px;
	font-size: 12px;
}

#xiangqingCenter ol li a {
	color: #fff;
}

#xiangqingCenter ul li {
	position: absolute;
	top: 40px;
}

#xiangqingCenter ul li img {}

#xiangqingCenter .xiangqingCO {
	width: 170px;
	background: #cb0147;
}

#xiangqingBottom {
	height: 200px;
}

#shopbtn {
	width: 158px;
	height: 48px;
	font-size: 20px;
	text-align: center;
	border: 1px solid #d6024b;
	background: #e5004f;
	color: #fff;
	cursor: pointer;
	position: absolute;
	left: 450px;
	top: 310px;
	z-index: 999;
}

.xiangqingImg {
	width: 220px;
	height: 220px;
	position: absolute;
}

.big-img {
	position: absolute;
	left: 105%;
	top: 0;
	width: 200px;
	height: 200px;
	overflow: hidden;
	z-index: 999;
	border: 1px solid #6A6565;
	display: none;
	background: #fff;
}

.big-img img {
	position: absolute;
	left: 0;
	top: 0;
}

.shopinfo {
	left: 60px;
	top: 50px;
	position: relative;
	width: 220px;
	height: 220px;
}

.shopinfo:hover .big-img {
	display: block;
}

#xiangqingTop-bottom .shopinfo:hover .hover {
	display: block;
}

.xiangqingP1 {
	position: absolute;
	width: 220px;
	text-align: center;
	top: 320px;
}

.xiangqingP2 {
	position: absolute;
	left: 100px;
	top: 270px;
	color: red;
}

.xiangqingP3 {
	position: absolute;
	left: 75px;
	top: 360px;
}

.xiangqing11 {
	position: absolute;
	left: 350px;
	top: 120px;
}

.xiangqingP4 {
	position: absolute;
	left: 250px;
	top: -10px;
	width: 350px;
	text-align: center;
	font-size: 20px;
}

.xiangqingP5 {
	position: absolute;
	left: 400px;
	top: 30px;
	color: red;
}

#jiage {
	position: absolute;
	left: 380px;
	top: 80px;
}

#xiangqingTop-bottom .hover {
	width: 50px;
	height: 50px;
	background: red;
	opacity: 0.3;
	position: absolute;
	z-index: 500;
	cursor: pointer;
	display: none;
}