body,
html {
	width: 100%;
}

html,
body,
ul,
li,
ol,
dl,
dd,
dt,
p,
h1,
h2,
h3,
h4,
h5,
h6,
form,
fieldset,
legend,
img {
	margin: 0;
	padding: 0;
}

fieldset,
img {
	border: 0;
}

img {
	display: block;
}

address,
caption,
cite,
code,
dfn,
th,
var {
	font-style: normal;
	font-weight: normal;
}

ul,
ol {
	list-style: none;
}

a {
	color: #666;
	text-decoration: none;
}

a:visited {
	color: #666;
}

a:hover,
a:active,
a:focus {
	color: #ff8400;
	text-decoration: none;
}

body,
html {
	font-family: "微软雅黑";
}


