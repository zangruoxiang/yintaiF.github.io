body,
html {
	width: 100%;
}

html,
body,
ul,
li,
ol,
dl,
dd,
dt,
p,
h1,
h2,
h3,
h4,
h5,
h6,
form,
fieldset,
legend,
img {
	margin: 0;
	padding: 0;
}

fieldset,
img {
	border: 0;
}

img {
	display: block;
}

address,
caption,
cite,
code,
dfn,
th,
var {
	font-style: normal;
	font-weight: normal;
}

ul,
ol {
	list-style: none;
}

a {
	color: #666;
	text-decoration: none;
}

a:visited {
	color: #666;
}

a:hover,
a:active,
a:focus {
	color: #ff8400;
	text-decoration: none;
}

body,
html {
	font-family: "微软雅黑";
}


/*-------------------------top left-----------------------*/

#top-box {
	width: 100%;
	background: #eee;
}

#top {
	height: 34px;
	width: 1190px;
	margin: 0 auto;
}

#top-left {
	float: left;
	margin-left: 10px;
}

#top-left img {
	float: left;
}

#top-left li {
	float: left;
	height: 34px;
	line-height: 34px;
	font-size: 12px;
}

#top-left a {
	border-right: 1px solid #ccc;
}

#top-left .top-left-left a {
	height: 14px;
	padding-right: 11px;
	margin-right: 10px;
}

#top-left .top-left-left img {
	margin-top: 7px;
	margin-right: 6px;
}

#top-left .top-left-center img {
	margin-top: 8px;
	margin-right: 6px;
}

#top-left .top-left-center {
	background: url(../img/top5.jpg) no-repeat right center;
	position: absolute;
	left: 142px;
	padding-left: 10px;
	z-index: 999;
}

#top-left .top-left-center a {
	border: none;
}

#top-left .top-left-center-right {
	float: right;
	margin-left: 8px;
	padding-top: 7px;
	padding-right: 6px;
}

#top-left .top-left-right a {
	border-right: 0;
}

#top-left .top-left-right {
	position: absolute;
	left: 230px;
}

#top-left .top-left-right img {
	margin-left: 14px;
	margin-top: 7px;
	margin-right: 5px;
}

#top-left .top-left-right .top-left-right-right {
	margin-left: 7px;
	margin-top: 15px;
	float: right;
}

#topnone1 {
	width: 114px;
	height: 124px;
	padding-left: 10px;
	padding-top: 15px;
	position: absolute;
	left: 142px;
	display: none;
}

#topnone2 {
	width: 148px;
	height: 163px;
	position: absolute;
	left: 230px;
	display: none;
}

#topnone3 ul li {
	width: 76px;
	height: 29px;
	font-size: 12px;
	color: #666;
	line-height: 29px;
	padding-left: 13px;
	border-bottom: 1px solid #dcdcdc;
	background: #fff;
}

#topnone3 ul li a:hover {
	border-bottom: 1px solid;
	border-color: #ff8400
}

#topnone3 ul {
	position: absolute;
	left: 991px;
	z-index: 1000;
}

#topnone3 {
	display: none;
}


